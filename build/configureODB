mkdir /Custom
cd Custom

create string MarkII&[1][128]
set MarkII& $PWD/index.html

create string combinedJS![1][128]
set combinedJS! $PWD/combined.js

create string combinedCSS![1][128]
set combinedCSS! $PWD/combined.css

mkdir Images
cd Images

mkdir GitHub-Mark-Light-64px.png
cd GitHub-Mark-Light-64px.png
create string Background[1][128]
set Background $PWD/../GitHub-Mark-Light-64px.png
cd ..

mkdir GRIFFIN_Logo_White_small.png
cd GRIFFIN_Logo_White_small.png
create string Background[1][128]
set Background $PWD/../GRIFFIN_Logo_White_small.png
cd ..